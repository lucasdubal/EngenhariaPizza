<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <!-- Parametros iniciais para a pagina teste.jsp -->   
        <servlet>    
  	<servlet-name>WebEntrega</servlet-name>    
  	<jsp-file>/login.jsp</jsp-file>    
  	
  	<init-param>      
  		<param-name>usuario</param-name>      
  		<param-value>funcionario</param-value>    
  	</init-param>    
        
        <init-param>      
  		<param-name>usuario2</param-name>      
  		<param-value>clientep</param-value>    
  	</init-param> 
        
        <init-param>      
  		<param-name>usuario3</param-name>      
  		<param-value>clientev</param-value>    
  	</init-param> 
         
  	<init-param>      
  		<param-name>senha</param-name>      
  		<param-value>1234</param-value>    
  	</init-param>  
        
        <init-param>      
  		<param-name>senha2</param-name>      
  		<param-value>1234</param-value>    
  	</init-param>
        
        <init-param>      
  		<param-name>senha3</param-name>      
  		<param-value>1234</param-value>    
  	</init-param>
        </servlet>
  <servlet-mapping>     
  	<servlet-name>WebEntrega</servlet-name>     
  	<url-pattern>/login.jsp</url-pattern>   
  </servlet-mapping>  
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
